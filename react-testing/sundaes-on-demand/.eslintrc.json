{
  "extends": ["plugin:testing-library/react", "react-app", "react-app/jest"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["testing-library", "jest-dom"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  }
}
